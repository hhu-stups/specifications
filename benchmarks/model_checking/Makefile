OPTS=
CSV_FILE=bench_default.csv
#OPTS= -p COMPRESSION TRUE -p OPERATION_REUSE TRUE
#CSV_FILE=bench_opreuse.csv

.PHONY=mc
mc:
	$(PROBCLI) $(MC_FILES) $(OPTS) --model-check -noass -nogoal -silent -timeout 90000 -machine_stats -bench_csv $(CSV_FILE) -version
	@echo "GIT SHA of specification repository:" >> $(CSV_FILE)
	@git rev-parse HEAD >> $(CSV_FILE)
	@echo "System and CPU information for $(TARGET_SYSTEM):" >> $(CSV_FILE)
	@uname -v >> $(CSV_FILE)
	@$(LSCPU) >> $(CSV_FILE)
	@echo "CSV output of MC Benchmark in $(CSV_FILE)"
	tail -n 14 $(CSV_FILE)

