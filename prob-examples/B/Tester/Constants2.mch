MACHINE Constants 
SETS
	ROUTEELEMENT = {route_el1,route_el2}
CONSTANTS
	PLACEELEMENT, FUNC
PROPERTIES
	PLACEELEMENT : POW(ROUTEELEMENT)  &  FUNC: ROUTEELEMENT >->> ROUTEELEMENT
VARIABLES
    pp
INVARIANT
   pp : PLACEELEMENT
INITIALISATION
   pp :: PLACEELEMENT
OPERATIONS
   cc <-- apply_func(pe) = PRE pe: PLACEELEMENT THEN cc := FUNC(pe) END
END