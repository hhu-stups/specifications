MACHINE DeterminedCheck
DEFINITIONS
  EXTERNAL_PREDICATE_IS_DETERMINED == (INTEGER);
  IS_DETERMINED(V) == (V=V);
  EXTERNAL_PREDICATE_IS_DETERMINED_INTO_FILE == (STRING*STRING*INTEGER);
  IS_DETERMINED_INTO_FILE(F,N,V) == (V=V)
CONSTANTS
  a,b
PROPERTIES
  a = 5 &
  b : 1..100 &
  IS_DETERMINED(a) & 
  not(IS_DETERMINED(b)) &
  IS_DETERMINED_INTO_FILE("detchecka.pl","a",a) &
  IS_DETERMINED_INTO_FILE("detcheckb.pl","b",b)
END